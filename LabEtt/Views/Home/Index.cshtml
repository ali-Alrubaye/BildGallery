@model IList<Datalager.Models.Photo>

@{
    ViewBag.Title = "Index";
}


<div class="container">
    <div class="row">
        <div class="jumbotron"><h2>Index</h2> </div>
        <div class="col-lg-6 col-sm-6 col-xs-12">
            <h3>Senaste uppladdade Bild</h3>
            @foreach (var result in Model)
            {
                if (Model.IndexOf(result) == Model.Count - 1)
                {
                    // this is the last item
                    <img src="@Url.Content("~/GalleryFolder/" + @result.PhotoPath)" alt="Kan int hita bilden" width="300" height="300">
                }
            }

        </div>
        <div class="col-lg-6 col-sm-6 col-xs-12">
            <div>
                <h3>Antal Bilder I Listan : @Model.Count()</h3>
                
            </div>
        </div>
    </div>
</div>

